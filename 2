#include <stdio.h>
#include <string.h>

int main() {
    char word[] = "butterfly";
    char result[4];
    int start = 6; 
    int end = 9;   

    int j = 0; 
    for (int i = start; i < end; i++) {
        result[j] = word[i];
        j++;
    }
    result[j] = '\0'; 

    printf("Результат: %s\n", result);

    return 0;
}
